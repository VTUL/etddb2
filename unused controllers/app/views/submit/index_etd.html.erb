<h1><center> Pending etds</center></h1>

<table>
  <tr>
    <th>Title</th>
    <th>URN</th>
    <th>Bound</th>
  </tr>

<% @etds.each do |etd| %>
  <tr>
    <td><%=h etd.title %></td>
    <td><%=h etd.urn %></td>
    <td><%=h etd.bound %></td>
    <td><%= link_to 'View', url_for(:controller => :submit, :action => :show_etd, :id => etd.id) %></td>
    <td><%= link_to 'Edit', url_for(:controller => :submit, :action => :edit_etd, :id => etd.id) %></td>
    <td><%= link_to 'Destroy', etd, :confirm => 'Are you sure?', :method => :delete_etd %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New etd', url_for(:controller => :submit, :action => :new_etd) %>
