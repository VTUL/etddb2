<h3>Other contents in this ETD:</h3>
<table>
  <tr>
    <th>Filename:</th>
    <th>Type:</th>
    <th>Size:</th>
    <th>Availability:</th>
    <th>Bound:</th>
    <th>Page Count:</th>
    <th>Duration:</th>
    <th>Dimensions:</th>
  </tr>
  <% @etd.contents.each do |content| %>
    <tr>
      <td><%=h content.content_file_name %></td>
      <td><%=h content.content_content_type %></td>
      <td><%=h content.content_file_size %></td>
      <td><%=h content.availability.name %></td>
      <td><%=h content.bound %></td>
      <td><%=h content.page_count %></td>
      <td><%=h content.dimensions %></td>
      <td><%=h content.duration %></td>
      <td><%= button_to 'Destroy', destroy_content_path(content), confirm: 'Are you sure?' %></td>
      <% if content.id != @content.id %>
              <td><%= link_to 'Show', content_path(content) %></td>
      <% end %>
    </tr>
  <% end %>
</table>
