<h2> <%=h @author.last_name %>'s ETD</h2>
<table>
  <tr>
    <th>Role</th>
    <th>First name</th>
    <th>Middle name</th>
    <th>Last name</th>
    <th>Email</th>
    <th>Pid</th>
    <th>Suffix</th>
  </tr>

<% @people.each do |person| %>
  <tr>
    <td><%=h person.role %></td>
    <td><%=h person.first_name %></td>
    <td><%=h person.middle_name %></td>
    <td><%=h person.last_name %></td>
    <td><%=h person.email %></td>
    <td><%=h person.pid %></td>
    <td><%=h person.suffix %></td>
    <td><%= link_to 'Show', url_for(:controller => 'people', :action => 'show', :id => person.id) %></td>
    <td><%= link_to 'Edit', url_for(:conteroller => 'people', :action => 'edit_committed', :id => person.id) %></td>
    <td><%= link_to 'Destroy', url_for(:confirm => 'Are you sure?', :method => :delete, :id => person.id) %></td>
  </tr>
<% end %>
</table>

<br />
<%= link_to 'Back', url_for(:controller => :submit, :action => :show_etd, :id => @etd.id)  %>
