<h3>My Etds</h3>
<table>
  <tr>
    <th>Title</th>
    <th>Abstract</th>
  </tr>

<% @my_etds.each do |etd| %>
  <tr>
    <td><%=h etd.title %></td>
    <td><%=h etd.abstract %></td>
    <td><%= link_to 'Show', etd %></td>
    <td><%= link_to 'Edit', edit_etd_path(etd) %></td>
    <td><%= link_to 'Destroy', etd, :confirm => 'Are you sure?', :method => :delete %></td>
    </td>
  </tr>
<% end %>
</table>
<br />
