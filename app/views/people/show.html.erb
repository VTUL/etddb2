<%= render 'profile' %>
<% if !@my_etds.empty? %>
  <%= render 'etd_items' %>
<% end %>
<% if current_person.id == @person.id %>
  <%= link_to 'New ETD', new_etd_path %>
  <br />
  <% if !@committee_etds.empty? %>
    <%= render 'committee_items' %>
  <% end %>
  <% if !@person.roles.where(group: 'Graduate School').empty? %>
    <%= render 'review_items' %>
  <% end %>
  <% if !@person.roles.where(group: 'Administration').empty? %>
    <%= render 'admin_items' %>
  <% end %>
<% end %>
<br />
<%= link_to 'Browse ETDs', etds_path %>
