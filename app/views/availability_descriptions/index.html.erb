<h1>Listing availability_descriptions</h1>

<table>
  <tr>
    <th>Availability</th>
    <th>Description</th>
  </tr>

<% @availability_descriptions.each do |availability_description| %>
  <tr>
    <td><%=h availability_description.availability %></td>
    <td><%=h availability_description.description %></td>
    <td><%= link_to 'Show', availability_description %></td>
    <td><%= link_to 'Edit', edit_availability_description_path(availability_description) %></td>
    <td><%= link_to 'Destroy', availability_description, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New availability_description', new_availability_description_path %>