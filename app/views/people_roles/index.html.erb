
<h1>Listing people_roles</h1>

<table>
  <tr>
    <th>Person</th>
    <th>Role</th>
    <th>ETD</th>
    <th>Vote</th>
  </tr>

<% @people_roles.each do |people_role| %>
  <tr>
    <td><%= h people_role.person_id %></td>
    <td><%= h people_role.role_id %></td>
    <td><%= h people_role.etd_id %></td>
    <td><%= h people_role.vote %></td>
    <td><%= link_to 'Show', people_role %></td>
    <td><%= link_to 'Edit', edit_people_role_path(people_role) %></td>
    <td><%= button_to 'Destroy', destroy_people_role_path(people_role), :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New people_role', new_people_role_path %>
