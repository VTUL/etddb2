<h1>Listing people_roles</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @people_roles.each do |person_role| %>
  <tr>
    <td><%=h person_role.name %></td>
    <td><%= link_to 'Show', person_role %></td>
    <td><%= link_to 'Edit', edit_person_role_path(person_role) %></td>
    <td><%= link_to 'Destroy', person_role, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New person_role', new_person_role_path %>
