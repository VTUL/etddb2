
<h1>Listing people_roles</h1>

<table>
  <tr>
    <th>Person</th>
    <th>Role</th>
    <th>ETD</th>
    <th>Vote</th>
  </tr>

<% @people_roles.each do |people_role| %>
  <tr>
    <td><%= people_role.person.name %></td>
    <td><%= people_role.role.name %></td>
    <% if !people_role.etd_id.nil? %>
    <td><%= link_to(people_role.etd.title.html_safe, etd_path(people_role.etd_id)) %></td>
    <% else %>
    <td></td>
    <% end %>
    <td><%= people_role.vote %></td>
    <td><%= link_to 'Show', people_role %></td>
    <td><%= link_to 'Edit', edit_people_role_path(people_role) %></td>
    <td><%= button_to 'Destroy', destroy_people_role_path(people_role), data: {confirm: 'Are you sure?'} %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New people_role', new_people_role_path %>
