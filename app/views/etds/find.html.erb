<h1>Find Person</h1>
<%= form_tag(find_committee_path(@etd), remote: true, id: "committee_search") do %>
  <%= label_tag(:name, "Member's Name") %>
  <%= text_field_tag(:name) %>
  <%= submit_tag("Find") %>
<% end %>
<br />
<%= form_tag(add_committee_path(@etd), id: "committee_add") do %>
  <div id="candidates">
  </div>
  <br />
  <%= submit_tag("Add to committee") %>
<% end %>
