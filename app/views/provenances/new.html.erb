<h1>New provenance</h1>

<% form_for(@provenance) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :creator %><br />
    <%= f.text_field :creator %>
  </p>
  <p>
    <%= f.label :notice %><br />
    <%= f.text_field :notice %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', provenances_path %>