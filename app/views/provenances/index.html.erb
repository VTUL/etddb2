<h1>Listing provenances</h1>
<table>
  <% @provenances.each do |prov| %>
    <tr>
      <td><%= get_name(prov.person) %> <%= prov.action %> <%= prov.model_type %> <%= prov.model_id %></td>
      <td><%= link_to 'Show', prov %></td>
      <td><%= link_to 'Edit', edit_provenance_path(prov) %></td>
      <td><%= button_to 'Destroy', destroy_provenance_path(prov), data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end %>
</table>
<br />
<%= link_to 'New provenance', new_provenance_path %>
