<h1>Listing privacy_statements</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @privacy_statements.each do |privacy_statement| %>
  <tr>
    <td><%= privacy_statement.name %></td>
    <td><%= privacy_statement.description %></td>
    <td><%= link_to 'Show', privacy_statement %></td>
    <td><%= link_to 'Edit', edit_privacy_statement_path(privacy_statement) %></td>
    <td><%= link_to 'Destroy', privacy_statement, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Privacy statement', new_privacy_statement_path %>
