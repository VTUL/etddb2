<%= stylesheet_link_tag 'paginate' %>
<h1 style="float:left;">Listing digital_objects</h1>
<div id="page_pref" class ="top_bar" style="margin-top:28px">
  <%= form_tag digital_objects_path, :method => 'get' do %>
    Results Per Page: <%= select_tag :per_page, options_for_select(%w[10 20 50 100], @per_page),
         :onchange => "this.form.submit();" %>
  <% end %>
</div>
<%= will_paginate @digital_objects %>
</br>
<table style="clear: both;">
  <tr>
    <th>Name</th>
  </tr>

<% @digital_objects.each do |digital_object| %>
  <tr>
    <td><%=h digital_object.name %></td>
    <td><%= link_to 'Show', digital_object %></td>
    <td><%= link_to 'Edit', edit_digital_object_path(digital_object) %></td>
    <td><%= button_to 'Destroy', destroy_digital_object_path(digital_object), data: {confirm: 'Are you sure?'} %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New digital_object', new_digital_object_path %>
